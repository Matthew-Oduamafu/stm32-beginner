#ifndef MAIN_H_
#define MAIN_H_

#define MASK(X) (1<<X)
#define MASK_POS(BITS, POS) (BITS<<POS)

typedef struct {
	uint32_t GPIOAEN : 1;
	uint32_t GPIOBEN : 1;
	uint32_t GPIOCEN : 1;
	uint32_t GPIODEN : 1;
	uint32_t GPIOEEN : 1;
	uint32_t GPIOFEN : 1;
	uint32_t GPIOGEN : 1;
	uint32_t GPIOHEN : 1;
	uint32_t GPIOIEN : 1;
} RCC_AHB1ENR_t;

typedef struct{

}GPIOx_MODER_t;

typedef struct{

}GPIOx_ODR_t;

#endif /* MAIN_H_ */
