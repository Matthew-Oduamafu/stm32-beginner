#ifndef MAIN_H_
#define MAIN_H_

#include <stdint.h>

#define MASK(X) (1<<X)
#define MASK_POS(BITS, POS) (BITS<<POS)

typedef struct {
	uint32_t gpioa_en : 1;
	uint32_t gpiob_en : 1;
	uint32_t gpioc_en : 1;
	uint32_t gpiod_en : 1;
	uint32_t gpioe_en : 1;
	uint32_t gpiof_en : 1;
	uint32_t gpiog_en : 1;
	uint32_t gpioh_en : 1;
	uint32_t gpioi_en : 1;

	uint32_t reserved1 : 3;

	uint32_t crc_en : 1;

	uint32_t reserved2 : 3;
	uint32_t reserved3 : 2;
	uint32_t bkpsram_en : 1;
	uint32_t resevered4 : 1;
	uint32_t ccmdat : 1;
	uint32_t dma1_en : 1;
	uint32_t dma2_en : 1;
	uint32_t resevered5 : 2;
	uint32_t ethmac_en : 1;
	uint32_t ethmactx_en : 1;
	uint32_t ethmacrx_en : 1;
	uint32_t ethmacptp_en : 1;
	uint32_t otghs_en : 1;

	OTGHSEN

} RCC_AHB1ENR_t;

typedef struct{

}GPIOx_MODER_t;

typedef struct{

}GPIOx_ODR_t;

#endif /* MAIN_H_ */
